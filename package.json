{
  "name": "philgeps",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "seed": "node seed-meilisearch.mjs",
    "search:stats": "node scripts/search-stats.mjs",
    "generate:sitemap": "cd data/gaa && python3 generate_sitemap.py --base-url https://transparency.bettergov.ph",
    "generate:sitemap:procurement": "node scripts/generate-procurement-sitemap.mjs --base-url https://transparency.bettergov.ph",
    "generate:sitemap:index": "node scripts/generate-sitemap-index.mjs --base-url https://transparency.bettergov.ph",
    "generate:sitemaps:all": "npm run generate:sitemap && npm run generate:sitemap:procurement && npm run generate:sitemap:index",
    "gaa:csv-to-parquet": "cd data/gaa && python3 gaa_csv_to_parquet.py --csv-dir data --output gaa.parquet",
    "gaa:create-aggregates": "cd data/gaa && python3 create_aggregates.py --parquet-file gaa.parquet --output-dir ../../public/data/gaa/aggregates",
    "gaa:import": "cd data/gaa && python3 import_gaa.py --parquet-file gaa.parquet --index-name gaa --batch-size 10000",
    "gaa:configure-search": "cd data/gaa && bash configure_meilisearch.sh",
    "gaa:pipeline": "npm run gaa:csv-to-parquet && npm run gaa:create-aggregates && npm run gaa:import && npm run gaa:configure-search",
    "gaa:reload": "npm run gaa:import && npm run gaa:configure-search"
  },
  "dependencies": {
    "@dr.pogodin/react-helmet": "^3.0.4",
    "@meilisearch/instant-meilisearch": "^0.29.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@tailwindcss/vite": "^4.1.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.553.0",
    "meilisearch": "^0.54.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-is": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "recharts": "^3.4.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2"
  }
}